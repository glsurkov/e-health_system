{
  "name": "server",
  "version": "1.0.0",
  "description": "Asset Transfer Basic REST API implemented in TypeScript",
  "main": "dist/index.js",
  "engines": {
    "node": ">=12"
  },
  "dependencies": {
    "@fidm/x509": "^1.2.1",
    "bullmq": "^1.47.2",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "env-var": "^7.0.1",
    "express": "^4.20.0",
    "express-validator": "^6.12.0",
    "fabric-ca-client": "^2.2.20",
    "fabric-network": "^2.2.20",
    "helmet": "^4.6.0",
    "http-status-codes": "^2.1.4",
    "ioredis": "^4.27.8",
    "long": "^5.2.3",
    "passport": "^0.6.0",
    "passport-headerapikey": "^1.2.2",
    "pino": "^6.11.3",
    "pino-http": "^5.5.0",
    "source-map-support": "^0.5.19",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@tsconfig/node12": "^12.1.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.12",
    "@types/ioredis": "^4.26.4",
    "@types/jest": "^27.4.1",
    "@types/node": "^12.20.55",
    "@types/passport": "^1.0.7",
    "@types/pino": "^6.3.8",
    "@types/pino-http": "^5.4.1",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "ioredis-mock": "^5.6.0",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.5",
    "pino-pretty": "^5.0.2",
    "prettier": "^3.5.3",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "~5.2.0"
  },
  "scripts": {
    "prebuild": "npm run lint",
    "build": "tsc",
    "clean": "rimraf ./dist",
    "format": "prettier --write \"{src,test}/**/*.ts\"",
    "generateEnv": "./scripts/generateEnv.sh",
    "lint": "eslint",
    "start": "node --require source-map-support/register ./dist",
    "start:dotenv": "node --require source-map-support/register --require dotenv/config ./dist",
    "start:dev": "ts-node --require dotenv/config --require source-map-support/register src/index.ts | pino-pretty",
    "dev": "cd src && npm run start:dev",
    "start:redis": "docker run -p 6379:6379 --name fabric-sample-redis -d redis --maxmemory-policy noeviction --requirepass \"${REDIS_PASSWORD}\"",
    "test": "jest"
  },
  "author": "Hyperledger",
  "license": "Apache-2.0",
  "private": true
}
