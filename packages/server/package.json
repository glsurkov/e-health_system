{
    "name": "server",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "start": "ts-node src/index.ts",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" --no-error-on-unmatched-pattern",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test:debug": "node --inspect -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest",
        "pre-commit": "lint-staged",
        "seed": "ts-node -r tsconfig-paths/register ./src/prisma/seed.ts && ts-node -r tsconfig-paths/register"
    },
    "dependencies": {
        "body-parser": "2.2.0",
        "cors": "2.8.5",
        "express": "5.1.0",
        "fabric-network": "2.2.20"
    },
    "devDependencies": {
        "@types/cors": "^2.8.17",
        "@types/express": "4.17.13",
        "@typescript-eslint/eslint-plugin": "8.1.0",
        "@typescript-eslint/parser": "8.1.0",
        "eslint": "9.9.0",
        "eslint-config-prettier": "^10.1.2",
        "eslint-plugin-import": "2.29.1",
        "eslint-plugin-prettier": "^5.2.6",
        "eslint-plugin-simple-import-sort": "12.1.1",
        "globals": "^15.9.0",
        "lint-staged": "15.2.8",
        "prettier": "^3.5.3",
        "source-map-support": "0.5.20",
        "supertest": "6.1.3",
        "ts-loader": "9.2.3",
        "ts-node": "^10.0.0",
        "tsconfig-paths": "4.2.0",
        "typescript": "5.5.4"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "preset": "ts-jest",
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage"
    },
    "engines": {
        "node": ">=22.6.0"
    }
}
